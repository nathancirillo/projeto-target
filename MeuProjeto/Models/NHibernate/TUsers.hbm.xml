<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="MeuProjeto" namespace="MeuProjeto.Models">
	<class name="TUsers" table="t_users" dynamic-update="true" >
		<cache usage="read-write"/>
		<id name="Id" column="Id" type="int">
			<generator class="native" />
		</id>
		<property name="UserName" />
		<property name="FullName" />
		<property name="Date" />
		<property name="Active" />
		<property name="Country" />
		<property name="Role" />
	</class>
</hibernate-mapping>